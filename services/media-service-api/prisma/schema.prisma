// Learn more about it in the docs:
// https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Media {
  id          Int    @id @default(autoincrement())
  title       String @unique
  type        String
  genre       String
  releaseYear Int
  rating      Int
}
